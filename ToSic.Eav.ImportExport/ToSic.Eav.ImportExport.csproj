<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netstandard2.0</TargetFrameworks>
	<Company>2sic Internet Solutions GmbH</Company>
	<Authors>2sic Internet Solutions GmbH</Authors>
	<Copyright>Copyright AGPL © 2sic 2022</Copyright>
    <Configurations>Debug;Release;Debug Oqtane;DebugOqtane;DebugDnn</Configurations>
    <RootNamespace>ToSic.Eav</RootNamespace>
    <Deterministic>true</Deterministic>
    <AutoGenerateBindingRedirects>False</AutoGenerateBindingRedirects>

	<!-- Don't generate assembly properties from this XML which should come from the core file, like version -->
	<GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
	<GenerateAssemblyFileVersionAttribute>false</GenerateAssemblyFileVersionAttribute>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net472'">
    <!--ATM we're still using v.0.81 which isn't available on nuget-->
    <!--<PackageReference Include="SharpZipLib" Version="0.86.0" />-->
    <Reference Include="SharpZipLib">
      <HintPath>..\Dependencies\SharpZipLib\SharpZipLib.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'netstandard2.0'">
    <PackageReference Include="SharpZipLib" Version="1.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\ToSic.Eav.Core\Properties\AssemblyInfoInherited.cs" Link="Properties\AssemblyInfoInherited.cs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\ToSic.Eav.Core\ToSic.Eav.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="ToSic.Eav.Convert\" />
  </ItemGroup>

</Project>
